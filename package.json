{
  "name": "vile-tslint",
  "version": "0.2.19",
  "description": "A vile plugin for TSLint",
  "main": "lib/index.js",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "author": "Brent Lintner <brent.lintner@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/forthright/vile-tslint/issues"
  },
  "homepage": "https://github.com/forthright/vile-tslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/forthright/vile-tslint.git"
  },
  "keywords": [
    "tslint",
    "typescript",
    "vile"
  ],
  "scripts": {
    "coffee": "coffee",
    "compile": "node bin/compile",
    "compile-dev": "coffee -cbo .test test",
    "dev": "bin/dev",
    "istanbul": "istanbul",
    "release-it": "release-it",
    "test": "_mocha --recursive -u bdd -R spec -t 2000 .test/spec.js",
    "test-cov": "node bin/test-cov",
    "lint-cov": "node bin/lint-cov",
    "update": "ncu -a",
    "vile": "vile p -x src:lib,test.coffee:.test.js -usi --nodecorations"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "lodash": "^4.17.4",
    "tslint": "^4.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "coffee-script": "~1.12.5",
    "istanbul": "~0.4.5",
    "mimus": "^0.2.5",
    "mocha": "~3.2.0",
    "npm-check-updates": ">=2.10.5",
    "release-it": "^2.7.1",
    "shelljs": "^0.7.7",
    "sinon": "~2.1.0",
    "sinon-chai": "~2.9.0",
    "typescript": "^2.2.2",
    "vile": ">=0.13.0",
    "vile-coffeelint": ">=0.1.14",
    "vile-comment": ">=0.1.17",
    "vile-coverage": ">=0.2.14",
    "vile-depcheck": ">=0.1.12",
    "vile-escomplex": "^0.1.2",
    "vile-eslint": ">=0.1.13",
    "vile-git": ">=0.5.1",
    "vile-language": ">=0.1.12",
    "vile-ncu": ">=0.2.1",
    "vile-nsp": ">=0.2.13",
    "vile-stat": ">=0.0.14"
  },
  "peerDependencies": {
    "vile": ">=0.13.0"
  }
}
